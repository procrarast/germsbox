{
	"manifest_version": 3,
	"name": "Germsbox",
	"version": "1.0",
	"permissions": [
		"tabs",
		"storage"
	],
	"host_permissions": [
		"https://germs.io/*"
	],	
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": [
				"https://germs.io/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"48": "gsDuh-48.png",
			"96": "gsDuh-96.png"
		}
	},	
	"icons": {
		"48": "gsDuh-48.png",
		"96": "gsDuh-96.png"
	},
	"commands": {
		"switchWindows": {
			"suggested_key": {
				"default": "Ctrl+Shift+A"
			},
			"description": "Simple and configurable Germs.io multiboxing script"
		}
	}
}